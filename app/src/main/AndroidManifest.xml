<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.maepaysoh.maepaysoh"
    >

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/AppTheme"
      >
    <activity
        android:name=".ui.HomeActivity"
        android:label="@string/app_name"
        >
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name=".ui.PartyListActivity"
        android:label="@string/PartyList"
        >
    </activity>
    <activity
        android:name=".ui.PartyDetailActivity"
        android:label="@string/party_detail_title"
        >
    </activity>
    <activity
        android:name=".ui.CandidateListActivity"
        android:label="@string/CandidateList"
        >
    </activity>
  </application>

</manifest>
